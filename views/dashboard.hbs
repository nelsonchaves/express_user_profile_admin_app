<h1>Hello Administrator!</h1>

<h3>Admin Profile:</h3>
<h4>{{FirstName}} {{LastName}}
    <br>
    Email: {{Email}}
    <br>
    Username: {{Username}}</h4>

<div class="userList1">
    <h2>List of Users</h2>
    <ul>
        {{#each users}}
        <li>{{FirstName}} {{LastName}}
            <a class="ViewUserBtn" href="/users/viewUser/{{UserId}}">View User</a>
            <form action="/users/viewUser/{{UserId}}" method="POST">
                <input type="submit" value="Delete User">
            </form>
        </li>
        <br>
        {{/each}}
    </ul>
</div>
<div class="userDetail">
    <a class="btn2" href="/users/login">LOG OUT</a>
</div>